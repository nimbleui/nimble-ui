{
  "name": "nimble-ui",
  "packageManager": "pnpm@9.5.0",
  "description": "Nimble UI monorepo",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=18.19.0",
    "pnpm": ">=8.13.1"
  },
  "workspaces": [
    "./packages/*",
    "apps"
  ],
  "scripts": {
    "build": "turbo build",
    "test": "turbo test",
    "clean": "turbo clean",
    "publint": "turbo publint",
    "dev": "turbo dev",
    "release": "turbo publint && changeset publish"
  },
  "author": "",
  "license": "MIT",
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "useTabs": false,
    "semi": true,
    "endOfLine": "auto"
  },
  "devDependencies": {
    "turbo": "^2.4.2"
  },
  "dependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@nimble-ui/lint-config": "workspace:^",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.14",
    "babel-plugin-annotate-pure-calls": "^0.5.0",
    "publint": "^0.3.7",
    "rimraf": "^6.0.1",
    "rollup": "^4.34.7",
    "rollup-plugin-dts": "^6.1.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.7"
  }
}
